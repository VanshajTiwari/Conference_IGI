<script>



let isPinging = false;

function measurePing() {
if (isPinging) {
  return;
}

isPinging = true;

    const startTime = Date.now();

    const range = Math.floor(Math.random() * 99) + 1;
    socket.emit('ping', range);

    socket.once('pong', (currTime) => {
        const ping = currTime - startTime + range;
        // console.log('ping', ping);
        updatePingValue(ping);
        isPinging = false;
    });
}

function updatePingValue(ping) {
document.getElementById('ping-value').textContent = ping + " ms";
switch(true) {
  case (ping <= 30):
    
    document.querySelector(".ping-signal").innerHTML = "Excellent";
    break;
  case (ping <= 60):

    document.querySelector(".ping-signal").innerHTML = "Good";
    break;
  case (ping <= 100):
    document.querySelector(".ping-signal").innerHTML = "Fair";
    break;
  default:
    document.querySelector(".ping-signal").innerHTML = "Poor";
}

}






setInterval(measurePing, 1000);
</script>


